{
  "name": "zondos",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "engines": {
    "node": "18.13.0"
  },
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec npx ts-node src/app.ts",
    "prebuild": "rimraf dist",
    "build": "npx tsc -p .",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "undo:db:migrate": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize db:seed:all",
    "undo:db:seed": "npx sequelize-cli db:seed:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "CCMaxwell",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
