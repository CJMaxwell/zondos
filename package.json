{
  "name": "zondos",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "engines": {
    "node": "14.18.0"
  },
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec npx ts-node src/app.ts",
    "prebuild": "rimraf dist",
    "build": "npx tsc -p .",
    "copy": "cp -r models dist/models && cp -r config dist/config",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build && npm run copy",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "undo:db:migrate": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize db:seed:all",
    "undo:db:seed": "npx sequelize-cli db:seed:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "CCMaxwell",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
